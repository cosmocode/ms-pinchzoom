{
  "name": "pinch-zoom-js",
  "version": "2.0.0",
  "description": "Browser library for multi-touch gestures to zoom and drag on any DOM element.",
  "author": "Manuel Stofer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuelstofer/pinchzoom.git"
  },
  "keywords": [
    "pinchzoom",
    "pinch",
    "zoom",
    "gesture",
    "touch",
    "drag"
  ],
  "main": "dist/pinch-zoom.umd.js",
  "module": "src/pinch-zoom.js",
  "files": [
    "dist/pinch-zoom.umd.js",
    "src/pinch-zoom.js"
  ],
  "devDependencies": {
    "babel-cli": "~6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "~1.6.1",
    "eslint-loader": "^1.9.0",
    "jest": "^22.1.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "scripts": {
    "prepublish": "npm run build",
    "dev": "webpack -d --progress --config webpack.dev.js",
    "watch": "webpack -dw --progress --config webpack.dev.js",
    "build": "webpack -p --progress --config webpack.prod.js",
    "test": "jest"
  }
}
